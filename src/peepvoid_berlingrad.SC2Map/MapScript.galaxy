    fixed lv_realDifference;

    // Automatic Variable Declarations
    // Variable Initialization

    // Implementation
    lv_realBase = Floor((lp_realNumber / lp_realIncrement));
    lv_realDifference = (lp_realNumber - (lv_realBase * lp_realIncrement));
