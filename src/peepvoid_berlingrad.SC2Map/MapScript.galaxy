            gf_RemovePlayerfromTeam(lv_intPlayer, false);
            gf_AddPlayertoTeam(lv_intPlayer, 0, false);
        }

        gv_recPlayers[lv_intPlayer].lv_boolPlayingMatch = false;
    }
    gf_InitializeFaceoff();
    return true;
}

