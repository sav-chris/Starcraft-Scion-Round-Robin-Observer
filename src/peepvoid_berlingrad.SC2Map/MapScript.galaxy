    // Variable Declarations
    int lv_intPlayer;
    text lv_txtMessage;

    // Automatic Variable Declarations
    // Variable Initialization
    lv_intPlayer = EventPlayer();

    // Conditions
    if (testConds) {
        if (!(((gv_recGameState.lv_boolGameInitComplete == true) && (gv_recGameState.lv_intCurrentGameState > 0) && (gv_recGameState.lv_boolMatchModeVotingActive == true)))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    if ((lv_intPlayer == gv_recGlobals.lv_intDebugPlayer)) {
        DialogControlSelectItem(gv_recUI.lv_diVotingMatchModePulldown, PlayerGroupAll(), DialogControlGetSelectedItem(gv_recUI.lv_diVotingMatchModePulldown, lv_intPlayer));
    }

    if ((gv_recGlobals.lv_boolAnnounceVotingChanges == true)) {
        lv_txtMessage = (TextWithColor(PlayerName(lv_intPlayer), libNtve_gf_ConvertPlayerColorToColor(PlayerGetColorIndex(lv_intPlayer, false))) + StringExternal("Param/Value/3D636FCD") + gv_recGlobals.lv_txtMatchModeNames[gv_recGlobals.lv_intMatchModeOrderList[DialogControlGetSelectedItem(gv_recUI.lv_diVotingMatchModePulldown, lv_intPlayer)]]);
        gf_CreateChatMessage(0, lv_txtMessage, 0, false, false);
    }

    gf_RefreshVotingMatchMode();
    if ((gv_recGameState.lv_intCurrentGameState == 1)) {
        gf_CalculateTeamInfo();
    }

    return true;
